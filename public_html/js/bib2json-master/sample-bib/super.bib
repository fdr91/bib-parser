@Article{Davies:1990:DOF,
  author =       "A. M. Davies and R. Proctor",
  title =        "Developing and optimizing a 3-{D}-spectral\slash
                 finite difference hydrodynamic model for the {Cray
                 X-MP}",
  journal =      j-COMP-FLUIDS,
  volume =       "18",
  number =       "3",
  pages =        "259--270",
  year =         "1990",
