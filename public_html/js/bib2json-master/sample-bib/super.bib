@MastersThesis{Konas:1991:PDEa,
  author =       "Pavlos Konas",
  title =        "Parallel discrete event simulation on shared memory
                 multiprocessors",
  type =         "Thesis (M.S.)",
  number =       "CSRD 1074; UILU-ENG-91-8035",
