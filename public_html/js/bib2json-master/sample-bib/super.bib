@Article{Seager:1986:PCG,
  author =       "Mark K. Seager",
  title =        "Parallelizing Conjugate Gradient for the {Cray X-MP}",
  journal =      j-PARALLEL-COMPUTING,
  volume =       "3",
  number =       "1",
  pages =        "35--47",
