@Article{Lorenz:2005:VTB,
  author =       "J. Lorenz and S. Kral and F. Franchetti and C. W.
                 Ueberhuber",
  title =        "Vectorization techniques for the {Blue Gene/L} double
                 {FPU}",
  journal =      j-IBM-JRD,
  volume =       "49",
  number =       "2/3",
  pages =        "437--446",
  month =        "????",
  year =         "2005",
  CODEN =        "IBMJAE",
