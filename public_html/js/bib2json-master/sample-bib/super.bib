@Article{Zenios:1986:NNP,
  author =       "Stavros A. Zenios and John M. Mulvey",
  title =        "Nonlinear Network Programming on Vector
                 Supercomputers: a Study on the {Cray X-MP}",
  journal =      j-OPER-RES,
  volume =       "34",
  number =       "5",
  pages =        "667--682",
