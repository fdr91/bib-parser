@Article{Yang:1998:SSE,
  author =       "Tao Yang and Cong Fu",
  title =        "Space\slash time-efficient scheduling and execution of
                 parallel irregular computations",
  journal =      j-TOPLAS,
  volume =       "20",
  number =       "6",
  pages =        "1195--1222",
  month =        nov,
  year =         "1998",
  CODEN =        "ATPSDT",
