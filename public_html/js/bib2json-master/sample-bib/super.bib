  DOI =          "http://doi.acm.org/10.1145/197405.197406",
  ISSN =         "0360-0300 (print), 1557-7341 (electronic)",
  bibdate =      "Thu Jun 19 09:46:22 MDT 2008",
  bibsource =    "http://www.acm.org/pubs/contents/journals/surveys/;
