@TechReport{Reeves:1994:SLP,
  author =       "Alyson Reeves and Mike Stillman",
  title =        "Smoothness of the lexicographic point",
  type =         "Technical report",
  number =       "SRC-TR-94-139",
  institution =  inst-SRC-IDA,
  address =      inst-SRC-IDA:adr,
  pages =        "13",
  day =          "16",
  month =        nov,
  year =         "1994",
  bibdate =      "Fri Aug 30 08:01:51 MDT 1996",
  bibsource =    "http://www.math.utah.edu/pub/tex/bib/super.bib",
  abstract =     "Let $k$ by any field, $\mbox{Hilb}^p(z)_{P_k^n}$ the
                 Hilbert scheme of subschemes of $P^n$ with Hilbert
                 polynomial $p(z)$. Macaulay showed [Ma26] that there
                 exists a unique saturated lexicographic deal $L$ such
                 that $k[xb0s,\ldots{},x_n]/L$ has Hilbert polynomial
                 $p(z)$. We show that the scheme corresponding to $L$ is
                 parametrized by a smooth point on the Hilbert scheme.
                 In the process we calculate the dimension of the unique
                 component through this point explicitly, and describe
                 explicitly the subscheme corresponding to the general
                 point of this component.",
